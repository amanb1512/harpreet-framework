package runTimePoymorphism;

public class ChildRunTimePolymorphismClass  extends ParentRunTimePolymorphismClass{
	
	public void myMethod() {System.out.println("i am a method of child class");
	}

	public static void main(String[] args) {
		
		ParentRunTimePolymorphismClass obj = new ChildRunTimePolymorphismClass();
		
		obj.myMethod();
		
		
		

	}

}
